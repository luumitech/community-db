    "immer": "^10.1.1",
    "next": "^14.2.4",
    "next-auth": "^4.24.7",
    "next-runtime-env": "^3.2.2",
    "next-themes": "^0.3.0",
    "object-hash": "^3.0.0",
    "omniconfig.js": "^1.2.0",
    "path-to-regexp": "^6.2.2",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.2",
    "react-icons": "^5.0.1",
    "react-infinite-scroll-hook": "^4.1.1",
