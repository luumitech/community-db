{
  "name": "community-db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn eslint",
    "lint-and-fix": "yarn eslint --fix",
    "eslint": "eslint . --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --max-warnings=0",
    "seed-db": "npx prisma db push --force-reset && npx prisma db seed",
    "graphql-codegen": "graphql-codegen --config codegen.ts",
    "prisma-generate": "npx prisma generate",
    "generate": "yarn prisma-generate && yarn graphql-codegen"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./prisma/seed.ts"
  },
