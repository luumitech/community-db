{
  "name": "community-db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:playwright": "dotenv -e .env.playwright -- next dev --turbopack",
    "email-dev": "shx --verbose cp .env.local ./node_modules/react-email/dist/preview && email dev --dir ./src/emails --port 3100",
    "build": "yarn generate && next build && yarn postbuild",
    "postbuild": "shx --verbose cp -r public .next/standalone/public && shx --verbose cp -r .next/static .next/standalone/.next/static",
    "start": "node .next/standalone/server.js",
    "start:local": "dotenv -e .env.local -- node .next/standalone/server.js",
    "lint": "next lint --max-warnings=0",
    "lint-and-fix": "yarn eslint --fix",
    "eslint": "eslint . --ext .ts,.tsx,.js --no-error-on-unmatched-pattern --max-warnings=0",
    "ts-check": "tsc --noEmit",
    "test": "jest --env=node --maxWorkers=1",
    "pw:open": "playwright test --ui",
    "pw:run": "playwright test --grep-invert '@screenshot'",
    "pw:screenshot": "playwright test --grep '@screenshot'",
    "update-prisma-db": "dotenv -e .env.local -- npx prisma db push",
    "reset-prisma-db": "dotenv -e .env.local -- npx prisma db push --force-reset",
    "seed-prisma-db": "dotenv -e .env.local -- npx prisma db seed",
    "seed-db": "yarn reset-prisma-db && yarn seed-prisma-db",
    "graphql-codegen": "dotenv -v NEXT_PHASE=phase-production-build -- graphql-codegen --require tsconfig-paths/register --config codegen.ts",
    "prisma-generate": "npx prisma generate",
    "generate": "yarn prisma-generate && yarn graphql-codegen",
    "ts-node": "node -r ts-node/register -r tsconfig-paths/register",
    "gen-favicon": "yarn ts-node ./src/app/lib/favicon/cli/gen-favicon",
    "semantic-release": "semantic-release"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --transpile-only ./prisma/seed.ts"
  },
